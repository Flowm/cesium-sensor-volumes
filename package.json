{
  "name": "cesium-sensor-volumes",
  "version": "1.33.0",
  "description": "A Cesium plugin for visualizing sensor volumes.",
  "homepage": "https://github.com/Flowm/cesium-sensor-volumes",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/Flowm/cesium-sensor-volumes.git"
  },
  "keywords": [
    "cesium"
  ],
  "main": "dist/cesium-sensor-volumes.js",
  "module": "dist/cesium-sensor-volumes.es.js",
  "dependencies": {
    "cesium": "1.77"
  },
  "devDependencies": {
    "browser-sync": "^2.18.8",
    "del": "^2.2.2",
    "electron": "1.6.5",
    "glsl-strip-comments": "^1.0.0",
    "gulp": "^4.0.2",
    "gulp-babel": "^8.0.0",
    "gulp-glsl": "^1.2.4",
    "gulp-xo": "^0.15.0",
    "jasmine-core": "^2.5.2",
    "karma": "^1.6.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-coverage": "^1.1.1",
    "karma-electron-launcher": "^0.2.0",
    "karma-jasmine": "^1.1.0",
    "karma-requirejs": "^1.1.0",
    "rollup": "^2.36.1",
    "rollup-plugin-string": "^3.0.0",
    "rollup-plugin-terser": "^7.0.2",
    "through2": "^2.0.3"
  },
  "scripts": {
    "build": "gulp build",
    "start": "gulp serve",
    "lint": "gulp lint",
    "test": "gulp test",
    "ci": "gulp ci",
    "gulp": "gulp"
  },
  "xo": {
    "globals": [
      "define"
    ],
    "esnext": false,
    "rules": {
      "capitalized-comments": "off",
      "func-names": "off",
      "import/no-amd": "off",
      "import/no-extraneous-dependencies": "off",
      "import/no-unresolved": "off",
      "import/no-webpack-loader-syntax": "off",
      "object-curly-spacing": [
        "error",
        "always",
        {
          "objectsInObjects": false,
          "arraysInObjects": false
        }
      ],
      "space-before-function-paren": [
        "error",
        "never"
      ]
    }
  }
}
